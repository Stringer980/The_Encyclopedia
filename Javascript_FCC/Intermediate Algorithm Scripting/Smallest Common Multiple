
function smallestCommons(arr) {
  let min = Math.min(...arr);
  let max = Math.max(...arr);
  let range = [];
  let arr1 =[]
  let arr2 = []
  let initial = 0

  for(let i=min; i<=max;i++){
    range.push(i)}

  while (initial<360360){
    initial ++
     arr1.push(initial*max)
    
  }return Math.min(...arr1.filter(a=>range.every(b=>a%b==0)))
} 

console.log(smallestCommons([1, 13]))
smallestCommons([1,5]);
	//

