
function smallestCommons(arr) {
  let min = Math.min(...arr);
  let max = Math.max(...arr);
  let range = [];
  let arr1 =[]
  for(let i=min; i<=max;i++){
    range.push(i)
  }
  
  let initial = 0
for (let i= 0; i<range.length;i++){
  let rangeNum = range[i]
  console.log(rangeNum)
  while (true){
    initial ++
 arr1.push(initial*max)
 if((initial*max)%1==0){break;} //find a replacement for 1
  }return arr1
}
}
console.log(smallestCommons([1,5]))
smallestCommons([1,5]);
